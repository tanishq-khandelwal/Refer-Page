generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Referral {
  id          Int      @id @default(autoincrement())
  user_name        String
  refree_name        String
  user_email       String
  refree_email   String
  referralCode String
  phoneno BigInt
  createdAt   DateTime @default(now())
}